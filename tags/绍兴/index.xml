	width=&#34;1159&#34;
	height=&#34;869&#34;
	srcset=&#34;http://localhost:1313/post/2021-10-16-shaoxing-in-rain/PXL_20211016_061644320_qingteng_yard_hu96023f3a6d1ea091bb1857289556bf3f_449051_480x0_resize_q75_box.jpg 480w, http://localhost:1313/post/2021-10-16-shaoxing-in-rain/PXL_20211016_061644320_qingteng_yard_hu96023f3a6d1ea091bb1857289556bf3f_449051_1024x0_resize_q75_box.jpg 1024w&#34;
	loading=&#34;lazy&#34;
	
